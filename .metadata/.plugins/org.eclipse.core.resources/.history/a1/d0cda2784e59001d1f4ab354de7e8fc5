package excercicio2;

import java.util.Scanner;



public class Exercicio3 {
	
	
	public static Scanner leia = new Scanner(System.in);
	public static void main(String[] args) {
		
		
		 int idade =0;
		 
		 
		 while(idade >=0  )
		    System.out.println(" Digite sua idade: ");
		    idade = leia.nextInt();

		    
	}	    
}